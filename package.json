{
  "name": "z1data-geocoder",
  "version": "1.0.3",
  "description": "A mapboxgl plugin for the Mapbox Search geocoder API",
  "main": "./src/index.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Khon123/geocoder.git"
  },
  "scripts": {
    "prepublish": "pnpm build",
    "start": "NODE_ENV=development budo example/index.js:example/bundle.js --live",
    "--build": "NODE_ENV=production mkdir -p dist && browserify -s Geocoder src/index.js > dist/geocoder.js && cp src/geocoder.css dist",
    "test": "NODE_ENV=test pnpm lint && browserify test/index.js | smokestack -b firefox | tap-status",
    "docs": "documentation build src/geocoder.js --shallow --format=md > API.md",
    "lint": "eslint --no-eslintrc -c .eslintrc src"
  },
  "keywords": [
    "directions",
    "search",
    "geocoder",
    "osm",
    "gl"
  ],
  "author": "Keaksey",
  "license": "ISC",
  "devDependencies": {
    "@types/lodash": "^4.14.184",
    "@types/node": "18.7.14",
    "@types/qs": "^6.9.7",
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.6",
    "babel-core": "^6.26.3",
    "babel-eslint": "^6.0.5",
    "babel-plugin-object-assign": "^1.2.1",
    "babel-plugin-transform-inline-environment-variables": "^6.8.0",
    "brfs": "^2.0.2",
    "browserify": "^17.0.0",
    "budo": "^11.8.4",
    "documentation": "^14.0.0",
    "eslint": "^8.23.1",
    "json-loader": "0.5.7",
    "lodash.once": "^4.1.1",
    "mapbox-gl": "^2.10.0",
    "smokestack": "^3.6.0",
    "tap-status": "^1.0.1",
    "tape": "^5.6.0",
    "transform-loader": "0.2.4",
    "typescript": "^4.8.3",
    "webpack": "5.74.0",
    "webworkify-webpack": "2.1.5"
  },
  "dependencies": {
    "@mui/material": "^5.10.3",
    "@mui/styled-engine-sc": "^5.10.3",
    "@reduxjs/toolkit": "^1.8.5",
    "@turf/turf": "^6.5.0",
    "axios": "^0.27.2",
    "babelify": "^10.0.0",
    "deep-assign": "^2.0.0",
    "event-pubsub": "^5.0.3",
    "lodash.debounce": "^4.0.8",
    "lodash.isequal": "^4.5.0",
    "lodash.template": "^4.5.0",
    "qs": "^6.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-final-form": "^6.5.9",
    "react-final-form-arrays": "^3.1.3",
    "react-infinite-scroll-component": "^6.1.0",
    "redux": "^4.2.0",
    "styled-components": "^5.3.5",
    "suggestions": "1.7.1",
    "turf-extent": "^1.0.4",
    "xtend": "4.0.2",
    "z1data-geocoder": "^1.0.1"
  }
}
